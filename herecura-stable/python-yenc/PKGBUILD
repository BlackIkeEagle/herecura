pkgname=python-yenc
pkgver=0.3
pkgrel=2
arch=('i686' 'x86_64')
license=('GPL')
pkgdesc="yenc decoder for python"
url="http://sabnzbd.sourceforge.net/"
depends=('python2')
source=(http://sabnzbd.sourceforge.net/yenc-$pkgver.tar.gz)
sha256sums=('fb04fea7c5821345608fa01728ce5356b6dfb2d3e469e59e3fd31b88f45fb313')

build() {
	cd $startdir/src/yenc-$pkgver
	python2 setup.py install --root=$startdir/pkg
}
