pkgver=1.1
pkgrel=1
pkgdesc="ArchCursor X Cursor Theme"
url="http://kde-look.org/content/show.php/Arch+Cursor+Theme+(simple)?content=135902"
depends=()
license=('GPL')
arch=('any')
source=("http://kde-look.org/CONTENT/content-files/135902-ArchCursorTheme.tar.gz")
sha256sums=('fae0657e0c13647c36dea628c0bce5a146f4b55829fa72d0a6e99e40ea49a26b')

build(){
	install -dm755 ${pkgdir}/usr/share/icons
	cp -a ${srcdir}/ArchCursorTheme ${pkgdir}/usr/share/icons/
}
