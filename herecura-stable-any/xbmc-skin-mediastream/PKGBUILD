pkgname=xbmc-skin-mediastream
pkgver=1.50
pkgrel=2
pkgdesc="MediaStream skin for XBMC"
arch=('any')
url="http://xbmc.org/skins/mediastream/"
source=("http://mirrors.xbmc.org/addons/skins/mediastream/mediastream-1.50.zip")
depends=('xbmc<=9.11')
provides=()
license=('GPL')
options=()

build() {
    mkdir -p $pkgdir/opt/xbmc/skin
    cd $srcdir
	cp -r MediaStream $pkgdir/opt/xbmc/skin/
}
