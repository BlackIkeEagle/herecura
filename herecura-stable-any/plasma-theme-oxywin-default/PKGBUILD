pkgname=plasma-theme-oxywin-default
pkgver=0.11.1
pkgrel=1
pkgdesc="Oxywin plasmatheme for KDE 4.x"
arch=('any')
license=('GPL')
url="http://kde-look.org/content/show.php/Oxywin?content=112004"
depends=('kdelibs' 'kdebase-workspace')
source=("http://kame2.de/KDE/oxywin/Oxywin-${pkgver}.tar.gz")
sha256sums=('24c3b10bc09df4e1165dd38434ec25b0c6cf9aca288bc94a909165423f634b3b')

build() {
	return
}

package() {
  install -dm755 ${pkgdir}/usr/share/apps/desktoptheme
  cp -a ${srcdir}/Oxywin ${pkgdir}/usr/share/apps/desktoptheme/oxywin
}
