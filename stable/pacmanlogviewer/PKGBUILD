pkgname=pacmanlogviewer
pkgver=1.2.1
pkgrel=1
pkgdesc="Inspect pacman log file."
arch=('i686' 'x86_64')
url=('https://opendesktop.org/content/show.php?content=150484')
license=('GPL')
depends=('qt4')
categories=('system')
source=("https://opendesktop.org/CONTENT/content-files/150484-$pkgname-$pkgver.tar.gz")

screenshot=('https://opendesktop.org/CONTENT/content-pre1/150484-1.png')

build() {
    cd "$srcdir/$pkgname-$pkgver"
    qmake-qt4
    make
}

package() {
    cd "$srcdir/$pkgname-$pkgver"
    make INSTALL_ROOT="$pkgdir" install
}

sha256sums=('a4428203e45f4afb0cfce23ff6409a2a68f6822c4d3f7ff427bffc53bf2d3dc0')
